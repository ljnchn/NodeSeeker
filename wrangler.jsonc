{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "nodeseeker",
  "compatibility_date": "2024-04-01",
  "compatibility_flags": ["nodejs_compat_v2"],
  "main": "./src/index.tsx",
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "nodeseeker-db",
      "database_id": "your-database-id-here"
    }
  ],
  "triggers": {
    "crons": ["0 */10 * * * *"]
  },
  "vars": {
    "ENVIRONMENT": "development"
  }
}